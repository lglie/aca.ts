// 所有RPC函数都需要放在该目录下
// 不能写在该index.ts页面中，除非该目录下只有这个页面
// RPC函数第一个参数必须为上下文（context），生成的前端同名函数没有这个参数
// 参照下面的写法：

import { $TbOper, Blog } from '..'

export async function example(ctx: any, name: $TbOper<'Blog__user'>['unique']) {
  return await Blog.user.findOne({ where: name, select: { id: true } })
}
